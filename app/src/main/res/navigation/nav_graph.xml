<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/SearchPhotoFragment">

    <fragment
        android:id="@+id/SearchPhotoFragment"
        android:name="com.development.clean.feature.searchphoto.SearchPhotoFragment"
        android:label="Search Photo"
        tools:layout="@layout/fragment_search_photo">

        <action
            android:id="@+id/action_to_another"
            app:destination="@+id/SearchPhotoFragment" />

        <argument
            android:name="query"
            app:argType="string"
            app:nullable="true" />
    </fragment>
</navigation>